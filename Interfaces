interface Animal
{
    void makeSound();
}

interface pet{
    void play();
}

class Dog implements Animal,pet{
    public void makeSound(){
        System.out.println("Sound");
    }
    public void play(){
        System.out.println("play");
    }
}

class Main{
    public static void main(String args[])
    {
        Dog d1=new Dog();
        d1.makeSound();
        d1.play();
    }
}
